<html>
	<head></head>
	<body>
		{% for img in images if img != null %}
			<div style="margin-bottom: 10px;">
				<div style="display:flex;">
					<img src="{{img.file}}" width="300"/>
				</div>

				<div style="display:flex;">
					{% for col in img.hex %}
						<div style="background-color:{{col}};width:40px;" title="#{{col}}">&nbsp;</div>
					{% endfor %}
				</div>

			</div>
		{% endfor %}
	</body>
</html>
