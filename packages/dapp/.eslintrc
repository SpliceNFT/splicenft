{
  "extends": "../../.eslintrc",
  "plugins": ["react"],
  "rules": {
    "no-debugger": "off",
    "@typescript-eslint/ban-ts-comment": "off",
    "@typescript-eslint/no-explicit-any": "off",
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".ts", ".tsx"]
      }
    ]
  }
}
